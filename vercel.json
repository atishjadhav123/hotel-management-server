{
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "npm run build",
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}